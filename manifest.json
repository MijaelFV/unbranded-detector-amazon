{
	"name": "Brand Detector",
	"description": "Brand Detector",
	"version": "0.1.0",
	"manifest_version": 2,
	"icons": {
		"128": "./icon.png"
	},
	"background": {
		"scripts": ["./background.js"]
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"https://www.amazon.com/*",
		"https://members.junglescout.com/*",
		"storage"
	]
}
